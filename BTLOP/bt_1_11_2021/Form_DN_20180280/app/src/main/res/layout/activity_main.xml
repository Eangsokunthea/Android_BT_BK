<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/UserName"
        android:layout_width="406dp"
        android:layout_height="49dp"
        android:text="HOTEN"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="5dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="409dp"
        android:layout_height="51dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="3dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="409dp"
        android:layout_height="51dp"
        android:text="MSSV"
        app:layout_constraintTop_toBottomOf="@+id/UserName"
        tools:layout_editor_absoluteX="2dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine3"
        android:layout_width="410dp"
        android:layout_height="51dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintTop_toBottomOf="@+id/UserName"
        tools:layout_editor_absoluteX="3dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="407dp"
        android:layout_height="50dp"
        android:text="NgaySinh"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="1dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine4"
        android:layout_width="407dp"
        android:layout_height="49dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="-1dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="410dp"
        android:layout_height="51dp"
        android:text="Gioi Tinh"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="1dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine5"
        android:layout_width="406dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="3dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="407dp"
        android:layout_height="53dp"
        android:text="Dia Chi Lien Lac"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="3dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine6"
        android:layout_width="408dp"
        android:layout_height="54dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="3dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="408dp"
        android:layout_height="57dp"
        android:text="So Dien Thoai"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="2dp" />

    <EditText
        android:id="@+id/editTextPhone2"
        android:layout_width="406dp"
        android:layout_height="57dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="405dp"
        android:layout_height="52dp"
        android:text="Email"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="3dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="405dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="408dp"
        android:layout_height="27dp"
        android:text="So Thich"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:layout_editor_absoluteX="3dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="99dp"
        android:layout_height="40dp"
        android:text="The Thao"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="98dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:text="CheckBox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="96dp"
        android:layout_height="38dp"
        android:layout_marginTop="8dp"
        android:text="Am Nhac"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox2" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3" />

</androidx.constraintlayout.widget.ConstraintLayout>